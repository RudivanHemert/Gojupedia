        "terms": {
          "age-uke": {
            "name": "Age Uke",
            "japanese": "上げ受け",
            "english": "Blocage Montant",
            "details": "Age Uke (Blocage Montant) est une technique de blocage montant fondamentale utilisée pour se défendre contre les attaques au visage ou au haut du corps. L'avant-bras se déplace vers le haut pour dévier l'attaque, tandis que l'autre main est retirée à la hanche en préparation d'une contre-attaque.",
            "videoFile": "age-uke"
          },
          "gedan-barai": {
            "name": "Gedan Barai",
            "japanese": "下段払い",
            "english": "Blocage de Balayage Descendant",
            "details": "Gedan Barai (Blocage de Balayage Descendant) est un blocage de balayage descendant qui défend contre les attaques basses. Le bras se déplace dans un arc descendant pour dévier les coups visant le bas du corps, avec l'autre bras qui se rétracte à la position de la hanche.",
            "videoFile": "gedan-barai"
          },
          "soto-uke": {
            "name": "Soto Uke",
            "japanese": "外受け",
            "english": "Blocage Extérieur",
            "details": "Soto Uke (Blocage Extérieur) est un blocage de l'extérieur vers l'intérieur utilisé pour dévier les attaques venant de l'extérieur. L'avant-bras se déplace de l'extérieur vers l'intérieur dans un chemin horizontal pour rediriger le coup de l'adversaire.",
            "videoFile": "soto-uke"
          },
          "uchi-uke": {
            "name": "Uchi Uke",
            "japanese": "内受け",
            "english": "Blocage Intérieur",
            "details": "Uchi Uke (Blocage Intérieur) est un blocage de l'intérieur vers l'extérieur utilisé pour dévier les attaques venant de l'intérieur. L'avant-bras se déplace de l'intérieur vers l'extérieur dans un chemin horizontal."
          },
          "shuto-uke": {
            "name": "Shuto Uke",
            "japanese": "手刀受け",
            "english": "Blocage Main Couteau",
            "details": "Shuto Uke (Blocage Main Couteau) est effectué en utilisant le bord de la main (shuto). Il peut être utilisé pour bloquer les attaques à la section médiane ou à la tête, souvent avec un mouvement circulaire."
          },
          "empi-uke": {
            "name": "Empi Uke",
            "japanese": "肘受け",
            "english": "Blocage Coude",
            "details": "Empi Uke (Blocage Coude) utilise le coude pour bloquer une attaque entrante, souvent efficace à courte portée et contre les coups au corps."
          },
          "kosa-uke": {
            "name": "Kosa Uke",
            "japanese": "交差受け",
            "english": "Blocage Croisé",
            "details": "Kosa Uke (Blocage Croisé) implique de croiser les poignets ou avant-bras pour créer une barrière contre une attaque. Il peut être effectué à diverses hauteurs (jodan, chudan, gedan)."
          },
          "juji-uke": {
            "name": "Juji Uke",
            "japanese": "十字受け",
            "english": "Blocage X",
            "details": "Juji Uke (Blocage X) utilise les bras croisés en forme de 'X' pour bloquer une attaque, typiquement un coup descendant ou un coup de pied. Les mains peuvent être ouvertes ou fermées."
          },
          "haiwan-uke": {
            "name": "Haiwan Uke",
            "japanese": "背腕受け",
            "english": "Blocage Bras Arrière",
            "details": "Haiwan Uke (Blocage Bras Arrière) utilise l'arrière de l'avant-bras pour bloquer une technique entrante. Il est souvent utilisé pour dévier les coups venant vers le côté du corps ou de la tête."
          },
          "osae-uke": {
            "name": "Osae Uke",
            "japanese": "押さえ受け",
            "english": "Blocage de Pression",
            "details": "Osae Uke (Blocage de Pression) est une technique qui presse vers le bas ou épingler une attaque de l'adversaire, souvent utilisée pour contrôler un membre de l'adversaire ou créer une ouverture."
          }
        }
      }
    },
